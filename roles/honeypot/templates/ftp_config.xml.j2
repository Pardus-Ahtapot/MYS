<?xml version='1.0' encoding='utf-8'?>
<appconfig>
        <dict name="enabled_handlers">
                <bool name="screen">False</bool>
                <bool name="elasticsearch">False</bool>
                <bool name="file">True</bool>
        </dict>
        <dict name="elasticsearch">
                <str name="index">honeypot</str>
                <str name="host">127.0.0.1</str>
                <int name="port">9200</int>
        </dict>
        <str name="sslcertprivate">{{honeypot_ftp_sslcertprivate}}</str>
        <str name="sslcertpublic">{{honeypot_ftp_sslcertpublic}}</str>
        <str name="filename">{{honeypot_ftp_filename}}</str>
        <str name="pubdir">{{honeypot_ftp_pubdir}}</str>
        <str name="passwdfile">{{honeypot_ftp_passwdfile}}</str>

        <int name="sslport">{{honeypot_ftp_sslport}}</int>
        <int name="port">{{honeypot_ftp_port}}</int>
</appconfig>

